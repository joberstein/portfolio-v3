{
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github",
        [
            "@qiwi/semrel-metabranch",
            {
                "publish": {
                    "action": "push",
                    "branch": "gh-pages",
                    "from": "./build",
                    "message": "Deploy ${nextRelease.gitTag}"
                }
            }
        ]
    ]
}